                },
                "key": {
                    "type": "string",
                    "format": "Mgr 密钥Key",
                    "example": "AQB+AsFew2rtHRAAwEpQAa1LOG9cYK7k66vtQA=="
                },
                "user": {
                    "type": "string",
                    "format": "用户名",
                    "example": "admin"
                }
            }
        },
        "web.ResponseBean": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "data": {
                    "type": "object"
                },
                "msg": {
